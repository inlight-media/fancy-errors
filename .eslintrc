{
  "env": {
    "node": true
  },
  "rules": {
    "strict": 0,
    "eol-last": 0,
    "curly": 2,
    "quotes": [2, "single"],
    "no-mixed-requires": [0, false],
    "no-use-before-define": [1, "nofunc"],
    "no-underscore-dangle": 0,
    "no-redeclare": 0,
    "consistent-return": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "valid-jsdoc": 1,
    "no-caller": 0
  }
}
